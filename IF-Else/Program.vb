Imports System
Module Program
    Sub Main()
        Console.WriteLine("IF-Else statement check ")

        Dim n As Integer

        console.write("Enter a number Positive/Negative :- ")
        n = console.readline()

        If (n >= 0) Then
            console.writeline("Positive Number !!")
        Else
            console.writeline("Negative Number !!")
        End If

        console.readline()
    End Sub
End Module
